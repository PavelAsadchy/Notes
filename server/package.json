{
  "name": "server",
  "version": "1.0.0",
  "description": "NodeJS Server",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node-dev --respawn src/server.ts",
    "lint": "eslint --ext .js,.ts --fix src/",
    "build": "tsc src/server.ts",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli --config src/database/ormconfig.ts",
    "migration:create": "npm run typeorm migration:create -- -n Migration -d src/migrations",
    "migration:generate": "npm run typeorm migration:generate -- -n CreateDatabase -d src/migrations",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PavelAsadchy/Notes.git"
  },
  "keywords": [
    "NodeJS",
    "REST",
    "Express"
  ],
  "author": "Pavel Asadchy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PavelAsadchy/Notes/issues"
  },
  "homepage": "https://github.com/PavelAsadchy/Notes#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^15.6.1",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
